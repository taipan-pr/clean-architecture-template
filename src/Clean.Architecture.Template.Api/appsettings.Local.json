{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "System.Net.Http.HttpClient": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "../../logs/log.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [],
    "Properties": {
      "Application": "Clean.Architecture.Template.Api",
      "Environment": "Local"
    }
  },
  "AllowedHosts": "*",
  "Seq": {
    "HostName": "",
    "ApiKey": ""
  }
}
